<!-- Modal -->
<%= image_tag post.photo.image.url(:medium), class: "modal-image" %>

<div class="modal-items">
  <div class="modal-item1">
    <p><%= post.created_at.strftime('%Y年 %m月 %d日') %></p>
    <p>投稿を削除</p>
  </div>
  <div class="modal-item2">
    <p><%= post.caption %></p>
  </div>
</div>


